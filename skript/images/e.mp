def common =
	pickup pencircle scaled 1pt;
	fill (-10,-20)--(0,-20)--(0,20)--(-10,20)--cycle
		withcolor (0.8,0.8,0.8);
	draw (0,-20)--(0,20);
	fill (400,-20)--(410,-20)--(410,20)--(400,20)--cycle
		withcolor (0.8,0.8,0.8);
	draw (400,-20)--(400,20);
enddef;

def threesprings(expr p, q, r) =

	spring((0,0), (100,0), c);
	spring((100,0), (200,0), c);
	spring((200,0), (300,0), c);
	spring((300,0), (400,0), c);

	pickup pencircle scaled 0.1pt;
	draw (100,0)--(100,-20);
	draw (200,0)--(200,-20);
	draw (300,0)--(300,-20);
	draw p--p shifted (0,-20);
	draw q--q shifted (0,-20);
	draw r--r shifted (0,-20);

	pickup pencircle scaled 1pt;
	drawarrow (100,-15)--p shifted (0,-15) withcolor red;
	drawarrow (200,-15)--q shifted (0,-15) withcolor red;
	drawarrow (300,-15)--r shifted (0,-15) withcolor red;

	mass((100,0),c,c);
	mass((200,0),c,c);
	mass((300,0),c,c);

	spring((0,0), p, black);
	spring(p, q, black);
	spring(q, r, black);
	spring(r, (400,0), black);
	mass(q, black, b);
	mass(p, black, b);
	mass(r, black, b);

	common;
enddef;

