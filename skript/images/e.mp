etex;

color c;
c = (0.9,0.9,0.9);
color b;
b = (0.8,0.8,0.8);

def mass(expr z, c, b) =
	pickup pencircle scaled 1pt;
	fill z shifted(-5,0) .. z shifted (0,-5) .. z shifted (5,0) 
		.. z shifted (0,5) .. cycle withcolor b;
	draw z shifted(-5,0) .. z shifted (0,-5) .. z shifted (5,0)
		.. z shifted (0,5) .. cycle withcolor c;
enddef;

def spring(expr p, q, c) =
	pickup pencircle scaled 1pt;
	draw p -- 0.125[p,q] -- 0.15[p,q] shifted(0,10)
                           -- 0.2 [p,q] shifted (0,-10)
                           -- 0.25[p,q] shifted (0, 10)
                           -- 0.3 [p,q] shifted (0,-10)
                           -- 0.35[p,q] shifted (0, 10)
                           -- 0.4 [p,q] shifted (0,-10)
                           -- 0.45[p,q] shifted (0, 10)
                           -- 0.5 [p,q] shifted (0,-10)
                           -- 0.55[p,q] shifted (0, 10)
                           -- 0.6 [p,q] shifted (0,-10)
                           -- 0.65[p,q] shifted (0, 10)
                           -- 0.7 [p,q] shifted (0,-10)
                           -- 0.75[p,q] shifted (0, 10)
                           -- 0.8 [p,q] shifted (0,-10)
                           -- 0.85[p,q] shifted (0, 10)
               -- 0.875[p,q] -- q withcolor c;
enddef;

